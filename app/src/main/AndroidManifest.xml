<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anugrah.aplikasiwisata">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiWisata">
        android:usesCleartextTraffic="true">
        <activity android:name=".admin.homeadmin"></activity>
        <activity android:name=".user.homeuser" />
        <activity
            android:name=".sign.FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen"
            android:theme="@style/Theme.AplikasiWisata.Fullscreen" />
        <activity
            android:name=".sign.signup"
            android:theme="@style/login" />
        <activity
            android:name=".sign.welcome"
            android:theme="@style/welcome" />
        <activity
            android:name=".sign.signin"
            android:theme="@style/login" />
        <activity
            android:name=".start.start"
            android:theme="@style/login" />
        <activity android:name=".splashscreen.splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>