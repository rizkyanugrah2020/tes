<resources>

    <style name="Widget.Theme.AplikasiWisata.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.Theme.AplikasiWisata.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

    <!-- Defined a new style with three items of color. -->
    <style name="welcome" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/brown</item>

        <!-- Defining that new color in ColorPrimaryDark -->
        <item name="colorPrimaryDark">@color/brown</item>
        <item name="colorAccent">@color/brown</item>
    </style>

    <!-- Defined a new style with three items of color. -->
    <style name="login" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/orange</item>

        <!-- Defining that new color in ColorPrimaryDark -->
        <item name="colorPrimaryDark">@color/orange</item>
        <item name="colorAccent">@color/orange</item>
    </style>
</resources>